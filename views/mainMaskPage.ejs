<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Face Off</title>
    <link rel="icon" href="/deeptechvision-favicon.png" />
    <link rel="stylesheet" href="/styles/maskPage.css" />
    <script
      src="https://cdn.jsdelivr.net/npm/@mediapipe/holistic@0.3.1620080860/holistic.min.js"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/@mediapipe/drawing_utils@0.3.1620080201/drawing_utils.min.js"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/@mediapipe/camera_utils@0.3.1620080164/camera_utils.min.js"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/@mediapipe/control_utils@0.3.1620080185/control_utils.min.js"
      crossorigin="anonymous"
    ></script>
    <link
      href="https://unpkg.com/tailwindcss@^2/dist/tailwind.min.css"
      rel="stylesheet"
    />
  </head>
  <body>
    <!-- NEW EJS FILE -->
    <!-- NEW EJS FILE -->
    <!-- NEW EJS FILE -->
    <!-- NEW EJS FILE -->
    <!-- no css req for video, hidden -->
    <video id="input`Video" autoplay style="display: none"></video><br />

    <div class="main">
      <div id="faceLogo">FaceOff<img src="" alt="" /></div>
      <div id="canvases">
        <canvas
          class="drawCanvas stack"
          width="960px"
          X-UA-Compatible
          height="540px"
          id="randoCanvas"
        ></canvas>

        <img id="facemask" class="maskOFF" src="./owl.svg" alt="" />

        <svg
          id="svg"
          width="960px"
          height="540px"
          class="stack"
          style="display: none"
        >
          <ellipse
            id="Leye"
            cx="40px"
            cy="40px"
            rx="8"
            ry="10"
            class="eye"
            fill="white"
            stroke="black"
            stroke-width="4"
          />
          <ellipse
            id="LEBall"
            cx="40px"
            cy="40px"
            rx="8"
            ry="10"
            class="eye"
            fill="black"
          />
          <ellipse
            id="Reye"
            cx="90px"
            cy="40px"
            rx="8"
            ry="10"
            class="eye"
            fill="white"
            stroke="black"
            stroke-width="4"
          />
          <ellipse
            id="REBall"
            cx="40px"
            cy="40px"
            rx="8"
            ry="10"
            class="eye"
            fill="black"
          />
          <ellipse id="LCheek" cx="90px " cy="40px" rx="8" ry="10" fill="red" />
          <ellipse id="RCheek" cx="90px " cy="40px" rx="8" ry="10" fill="red" />
          <path
            id="upLip"
            d=" M 10 80 Q 40 30 70 80"
            stroke="red"
            stroke-width="3"
            fill="pink"
          />
          <path
            id="lowLip"
            d="M 10 80 Q 40 120 70 80"
            stroke="red"
            stroke-width="3"
            fill="pink"
          />
          <path
            id="leftCheek"
            d="M 10 100 Q 40 30 70 100"
            stroke="x#C0C0C0	"
            stroke-width="3"
            stroke-linecap="round"
            fill="transparent"
          />
          <path
            id="rightCheek"
            d="M 10 100 Q 40 30 70 100"
            stroke="#C0C0C0	"
            stroke-width="3"
            stroke-linecap="round"
            fill="transparent"
          />
          <path
            id="lineNose"
            d="M 10 100 Q 40 30 70 100"
            stroke="brown"
            stroke-width="4"
            stroke-linecap="round"
            fill="transparent"
            style="display: none"
          />
          <path
            id="leftBrow"
            d="M 20 200 Q 40 30 80 100"
            stroke="black"
            stroke-width="3"
            stroke-linecap="round"
            fill="transparent"
          />
          <path
            id="rightBrow"
            d="M 20 200 Q 40 30 80 100"
            stroke="black"
            stroke-width="3"
            stroke-linecap="round"
            fill="transparent"
          />
          <path
            id="leftExp1"
            d="M 20 200 Q 40 30 80 100"
            stroke="#C0C0C0	"
            stroke-width="3"
            stroke-linecap="round"
            fill="transparent"
          />
          <path
            id="rightExp1"
            d="M 20 200 Q 40 30 80 100"
            stroke="#C0C0C0	"
            stroke-width="3"
            stroke-linecap="round"
            fill="transparent"
          />
          <path
            id="REDnose"
            stroke="black"
            stroke-width="5"
            stroke-linecap="round"
            fill="red"
            d="M 100 100 q 25 -5 50 0   10 10 -25 25   -35 -15 -25 -25"
          />
        </svg>

        <canvas
          id="handCanvas"
          class="stack"
          width="960px"
          X-UA-Compatible
          height="540px"
        ></canvas>

        <canvas
          id="finalCanvas"
          class="stack"
          style="margin-top: 500px; display: none"
          width="960px"
          X-UA-Compatible
          height="540px"
        ></canvas>
        <div style="width: 512px; height: 288px" id="offsetDiv"></div>
      </div>
      <div id="masks">
        <div id="mask1">
          <img id="iconimage" src="/icons/owl-icon.PNG" alt="" />
        </div>
        <div id="mask2">
          <img id="iconimage" src="/icons/yellow-icon.PNG" alt="" />
        </div>
        <div id="mask3">
          <img id="iconimage" src="/icons/blue-icon.PNG" alt="" />
        </div>
      </div>
      <div id="controls">
        <div class="button" id="handBtn">
          <img src="/icons/hand-48.png" alt="" />
        </div>

        <div class="button" id="recordBtn">
          <img src="/icons/play-52.png" alt="" />
        </div>

        <div class="button" id="logoutBtn">
          <img
            src="/icons/logout-left-52.png"
            alt=""
            style="-webkit-transform: scaleX(-1); transform: scaleX(-1)"
          />
        </div>
      </div>
    </div>
  </body>
  <script src="js/setPage.js"></script>
  <!-- <script src="/js/maskScript.js"></script> -->
  <script src="js/mainMask.js"></script>
  <script src="/js/record.js"></script>
</html>
